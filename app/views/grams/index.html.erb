Index

<% @grams.each do |gram| %>
    <div>
        <div>
            <%= link_to(gram.title, gram) %>
        </div>
        
        <%= time_ago_in_words(gram.created_at) + ' ago'%>
        <%= gram.description %>
        <%= gram.location_code %>
        
        <div>
            <%= link_to('Delete', gram, method: :delete)%>
        </div>
    </div><hr>
<% end %>